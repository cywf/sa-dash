# Finnhub API Key
# Get your free key at: https://finnhub.io/
# Free tier: 60 calls/minute
VITE_FINNHUB_API_KEY=your_api_key_here

# Intel Sources Feature Flags (all default to false)
PUBLIC_FEATURE_INTEL_SOURCES=false
PUBLIC_FEATURE_ADSB=false
PUBLIC_FEATURE_SATELLITES=false
PUBLIC_FEATURE_SPECTRUM=false
PUBLIC_FEATURE_SHODAN=false
PUBLIC_FEATURE_WIGLE=false
PUBLIC_FEATURE_SEISMIC=false
PUBLIC_FEATURE_RF_PROP=false
PUBLIC_FEATURE_AURORA=false

# Shodan API Key (required for Shodan panels)
# Get your key at: https://account.shodan.io/
# Free tier: 100 results/month, 1 query credit
SHODAN_API_KEY=

# WiGLE API Credentials (required for WiGLE panels)
# Get your credentials at: https://wigle.net/account
# Free tier: Unlimited queries with rate limits
WIGLE_API_NAME=
WIGLE_API_TOKEN=
